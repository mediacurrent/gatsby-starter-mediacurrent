{
  "name": "gatsby-starter-mediacurrent",
  "private": true,
  "description": "Mediacurrent's full-featured Gatsby starter",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop -H 0.0.0.0",
    "storybook": "NODE_ENV=test start-storybook -s ./static -p 6006",
    "build": "npm run build:gatsby && npm run build:storybook",
    "build:gatsby": "gatsby build",
    "build:storybook": "NODE_ENV=test build-storybook --quiet -s ./static -c .storybook -o public/storybook",
    "copy-static-queries": "cp -r ./public/page-data/sq/d ./public/static",
    "prestorybook": "npm run copy-static-queries",
    "prebuild:storybook": "npm run copy-static-queries",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "serve": "gatsby serve",
    "serve:prod": "gatsby build && gatsby serve",
    "serve:storybook": "npx http-server ./public/storybook",
    "clean": "gatsby clean",
    "lint": "npm run lint:sass && npm run lint:js",
    "lint:staged": "lint-staged",
    "lint:js": "jest --config ./jest/jest.lint.js",
    "lint:sass": "jest --config ./jest/jest.scss.js",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "npm run test:coverage -- -u",
    "component": "plop component",
    "template": "plop template",
    "page": "plop page"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md,mdx,json}": "prettier --write",
    "*.scss": "npm run lint:sass"
  },
  "jest-runner-stylelint": {
    "cliOptions": {
      "fix": true
    }
  },
  "dependencies": {
    "@loadable/component": "^5.15.0",
    "@mediacurrent/gatsby-plugin-silence-css-order-warning": "^1.0.0",
    "classnames": "^2.3.1",
    "focus-visible": "^5.2.0",
    "gatsby": "^3.9.1",
    "gatsby-image": "^2.7.0",
    "gatsby-plugin-advanced-sitemap": "^2.0.0",
    "gatsby-plugin-env-variables": "^2.1.0",
    "gatsby-plugin-gatsby-cloud": "2.9.1",
    "gatsby-plugin-image": "^1.9.0",
    "gatsby-plugin-loadable-components-ssr": "^3.2.0",
    "gatsby-plugin-react-helmet": "^4.9.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.6.8",
    "gatsby-plugin-sass": "^4.9.0",
    "gatsby-plugin-sharp": "^3.9.0",
    "gatsby-remark-prismjs": "^5.6.0",
    "gatsby-source-drupal": "^4.9.0",
    "gatsby-source-filesystem": "^3.9.0",
    "gatsby-transformer-remark": "^4.6.0",
    "gatsby-transformer-sharp": "^3.9.0",
    "node-fetch": "^2.6.1",
    "node-sass": "^6.0.1",
    "prismjs": "^1.24.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "unfetch": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@loadable/babel-plugin": "^5.13.2",
    "@mediacurrent/eslint-config-react": "^0.0.1",
    "@mediacurrent/prettier-config": "^0.0.1",
    "@mediacurrent/storybook-config-gatsby": "^0.0.1",
    "@mediacurrent/stylelint-config": "0.0.2",
    "@storybook/addon-a11y": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/addon-storyshots": "^6.3.4",
    "@storybook/builder-webpack5": "^6.3.4",
    "@storybook/manager-webpack5": "^6.3.4",
    "@storybook/react": "^6.3.4",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^3.0.0",
    "babel-plugin-remove-graphql-queries": "^2.12.0",
    "babel-preset-gatsby": "^0.8.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^2.2.0",
    "jest": "^27.0.6",
    "jest-runner-eslint": "^0.10.1",
    "jest-runner-stylelint": "^2.3.7",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^11.0.1",
    "plop": "^2.7.4",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.20.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
